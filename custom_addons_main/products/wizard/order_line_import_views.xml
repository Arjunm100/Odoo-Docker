<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="order_line_import_view_form" model="ir.ui.view">
        <field name="name">order.line.import.view.form</field>
        <field name="model">order.line.import</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="file"/>
                    </group>
                </sheet>
                <footer>
                    <button name="action_import_lines" type="object" string="Import" class="btn-primary"/>
                    <button special="cancel" string="Cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>